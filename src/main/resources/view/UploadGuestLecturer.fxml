<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.francis.my.sfupa.JavaFX.Controller.GuestLecturer"
            prefHeight="400.0" prefWidth="600.0">

    <VBox spacing="10" layoutX="50" layoutY="50">
        <Label text="Upload Guest Lecturer CSV" />

        <ComboBox fx:id="semesterCombo" promptText="Select Semester"/>
        <ComboBox fx:id="courseCombo" promptText="Select Course"/>
        <TextField fx:id="yearField" promptText="Enter Year"/>
        <TextField fx:id="guestLecturerField" promptText="Enter Guest Lecturer Name"/>


        <Button text="Back" onAction="#handleBack"/>
    </VBox>

</AnchorPane>
